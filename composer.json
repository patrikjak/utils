{
    "name": "patrikjak/utils",
    "description": "A simple utility package for Laravel",
    "keywords": ["laravel", "utils", "utility"],
    "version": "0.1.2",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Patrikjak\\Utils\\": "src/",
            "Patrikjak\\Utils\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Patrik Jakab"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.25"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "slevomat/coding-standard": "^8.14",
        "phpunit/phpunit": "^11.4"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
