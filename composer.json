{
    "name": "patrikjak/utils",
    "description": "A simple utility package for Laravel",
    "version": "0.1.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Patrikjak\\Utils\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Patrik Jakab"
        }
    ],
    "require": {
        "laravel/framework": "^11.25"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "slevomat/coding-standard": "^8.14"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
