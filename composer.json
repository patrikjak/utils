{
    "name": "patrikjak/utils",
    "description": "A simple utility package for Laravel",
    "keywords": ["laravel", "utils", "utility", "tables"],
    "version": "2.6.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Patrikjak\\Utils\\": "src/",
            "Patrikjak\\Utils\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Patrik Jakab"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.1.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "slevomat/coding-standard": "^8.14",
        "phpunit/phpunit": "^11.4",
        "spatie/phpunit-snapshot-assertions": "^5.1",
        "orchestra/testbench": "^10.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
